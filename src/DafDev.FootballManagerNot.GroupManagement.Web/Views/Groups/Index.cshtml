@model IEnumerable<Group>
@{
    ViewData["Title"] = "Index";
}
<h2>Index</h2>

<p>Here are the groups</p>

<ul>
@foreach (var group in Model)
{
    <li>@group.Name <a asp-controller="Groups" asp-action="Details" asp-route-id=@group.Id>Edit</a></li>
}    
</ul>

<a asp-controller="Groups" asp-action="Create">Create</a>